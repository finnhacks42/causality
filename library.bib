journal = {Am. J. Epidemiol.},
keywords = {Bias (Epidemiology),Case-Control Studies,Central Nervous System Neoplasms,Central Nervous System Neoplasms: epidemiology,Central Nervous System Neoplasms: etiology,Child,Diet,Electromagnetic Fields,Electromagnetic Fields: adverse effects,Epidemiologic Methods,Glioma,Glioma: epidemiology,Glioma: etiology,Humans,Leukemia,Leukemia: epidemiology,Leukemia: etiology,Likelihood Functions,Logistic Models,Matched-Pair Analysis,Odds Ratio,Regression Analysis,Risk Assessment},
month = mar,
number = {5},
pages = {531--9},
pmid = {10707923},
title = {{Problems due to small samples and sparse data in conditional logistic regression analysis.}},
