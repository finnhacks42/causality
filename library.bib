issn = {1532-0480},
journal = {J. Biomed. Inform.},
keywords = {Bayes Theorem,Causality,Drug-Related Side Effects and Adverse Reactions,Electronic Health Records,Epidemiology,Humans,Individualized Medicine,Medical Informatics,Risk Factors},
