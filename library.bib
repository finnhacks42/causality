issn = {1879-3134},
journal = {Math. Biosci.},
keywords = {Bayes Theorem,Cluster Analysis,Gene Regulatory Networks,Gene Regulatory Networks: genetics,Genomics,Genomics: methods,Humans,Models, Genetic},
