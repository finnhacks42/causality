file = {:home/finn/phd/papers/orgainized/2014/Hern\'{a}ndez-Lobato et al. - 2014.pdf:pdf},
month = sep,
pages = {27},
title = {{Non-linear Causal Inference using Gaussianity Measures}},
url = {http://arxiv.org/abs/1409.4573},
volume = {1},
year = {2014}
}
