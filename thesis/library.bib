doi = {10.1111/1468-0262.00442},
file = {:home/finn/phd/papers/papers{\_}pdf/efficient{\_}estimation{\_}of{\_}average{\_}treatment{\_}effects{\_}using{\_}the{\_}estimated{\_}propensity{\_}score.pdf:pdf},
