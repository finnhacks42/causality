author = {Auer, P. and Cesa-Bianchi, N. and Freund, Y. and Schapire, R.E.},
doi = {10.1109/SFCS.1995.492488},
file = {:home/finn/phd/papers/papers{\_}pdf/refs4462.pdf:pdf},
isbn = {0-8186-7183-1},
issn = {0272-5428},
