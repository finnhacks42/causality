keywords = {Causality,Humans,Psychological Theory,Psychology,Psychology: methods,Research Design},
month = {mar},
number = {1},
pages = {18--37},
pmid = {20230100},
title = {{Campbell's and Rubin's perspectives on causal inference.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20230100},
volume = {15},
